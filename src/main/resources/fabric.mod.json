{
  "schemaVersion": 1,
  "id": "helpfulcommands",
  "version": "${version}",

  "name": "Helpful Commands",
  "description": "Minecraft mod that adds lots of useful commands to make your experience better, more optimized, and flexible. Easily configurable, feature-rich, and interactive.",
  "authors": [
    "ThatsNotM3"
  ],
  "contact": {
    "homepage": "https://github.com/ThatsNotM3/HelpfulCommands",
    "sources": "https://github.com/ThatsNotM3/HelpfulCommands",
    "issues": "https://github.com/ThatsNotM3/HelpfulCommands/issues"
  },

  "license": "MIT License",
  "icon": "assets/helpfulcommands/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.thatsnotm3.helpfulcommands.HelpfulCommands"
    ],
    "client":[
      "net.thatsnotm3.helpfulcommands.HelpfulCommandsClient"
    ]
  },
  "mixins": [
    "helpfulcommands.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.22",
    "fabric-api": "*",
    "minecraft": "~1.20.2",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}